{
	"name": "vscode-chameleon",
	"displayName": "Chameleon",
	"description": "Randomize the theme and font on startup",
	"version": "0.0.1",
	"publisher": "timdeschryver",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/timdeschryver/vscode-chameleon.git"
	},
	"homepage": "https://github.com/timdeschryver/vscode-chameleon/blob/main/README.md",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"theme",
		"font"
	],
	"extensionKind": [
		"ui"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "chameleon.switchTheme",
				"title": "Chameleon: Switch to another theme"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/babel__core": "^7.1.12",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
